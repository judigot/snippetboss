{
  "name": "snippetmaster",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm install && pnpm prisma generate && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "seed": "pnpm dotenv -e .env.local -- pnpm prisma db seed",
    "prisma-push-schema": "pnpm dotenv -e .env.local -- pnpm prisma db push && pnpm prisma generate",
    "prisma-pull-schema": "pnpm dotenv -e .env.local -- pnpm prisma db pull && pnpm prisma generate"
  },
  "prisma": {
    "seed": "ts-node --compilerOptions {\"module\":\"CommonJS\"} ./src/prisma/seed/seed.ts",
    "schema": "src/prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^5.8.1",
    "axios": "^1.6.5",
    "bootstrap": "^5.3.2",
    "dotenv": "^16.3.1",
    "next": "14.0.4",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.3",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "autoprefixer": "^10.0.1",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8",
    "prettier": "^3.2.2",
    "prisma": "^5.8.1",
    "styled-components": "^6.1.8",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite-tsconfig-paths": "^4.2.3",
    "vitest": "^1.2.0"
  }
}
